# 개발 환경 설정 예시
# 사용법: cp env.development.example .env (루트 디렉토리에)
#        또는 Docker Compose 사용 시 docker-compose --env-file env.development.example up

# 환경
ENVIRONMENT=development

# 데이터베이스
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=swu_chatbot
POSTGRES_PORT=5432
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/swu_chatbot

# Redis
REDIS_PORT=6379
REDIS_URL=redis://localhost:6379/0
REDIS_PASSWORD=

# API Keys
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# 백엔드 설정
BACKEND_PORT=8000
LOG_LEVEL=DEBUG

# CORS (개발 환경)
CORS_ORIGINS=http://localhost:3000,http://localhost:3001,http://127.0.0.1:3000

# 레이트 리미팅 (개발 환경 - 느슨한 제한)
RATE_LIMIT_ENABLED=true
RATE_LIMIT_PER_MINUTE=200

# 캐시 설정
CACHE_ENABLED=true
EMBEDDING_CACHE_TTL=86400
SEARCH_CACHE_TTL=3600
API_CACHE_TTL=300

# 프론트엔드 설정
FRONTEND_PORT=3000
NODE_ENV=development
NEXT_PUBLIC_API_URL=http://localhost:8000

# Nginx (선택사항)
NGINX_PORT=80

# Gunicorn 설정 (개발 환경에서는 사용 안 함)
WORKERS=2
MAX_REQUESTS=1000
MAX_REQUESTS_JITTER=100
